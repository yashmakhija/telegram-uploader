{
  "name": "telegram-file-uploader",
  "version": "1.0.0",
  "description": "Telegram bot for uploading files and providing proxy download URLs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --esm src/index.ts",
    "postinstall": "npx prisma generate",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prod:start": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "file",
    "upload",
    "proxy"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mtproto/core": "^6.3.0",
    "@prisma/client": "^6.14.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.10",
    "@types/uuid": "^9.0.8",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.5.1",
    "fs-extra": "^11.3.1",
    "helmet": "^7.2.0",
    "morgan": "^1.10.1",
    "telegraf": "^4.16.3",
    "telegram": "^2.26.22",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "eslint": "^8.57.0",
    "prisma": "^6.14.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
