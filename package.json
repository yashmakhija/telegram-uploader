    "@types/morgan": "^1.9.10",
    "@types/uuid": "^9.0.8",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "morgan": "^1.10.1",
    "telegraf": "^4.16.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
